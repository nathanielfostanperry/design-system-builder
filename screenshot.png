<?xml version="1.0" encoding="UTF-8"?>
<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <rect width="1200" height="630" fill="#f8fafc"/>

  <!-- Header -->
  <text x="600" y="80" font-family="Arial" font-size="40" font-weight="bold" text-anchor="middle" fill="#1e293b">Design System Color Generator</text>

  <!-- Color input section -->
  <rect x="250" y="120" width="700" height="120" rx="8" fill="#ffffff" stroke="#e2e8f0" stroke-width="2"/>

  <!-- Primary color -->
  <rect x="300" y="150" width="120" height="60" rx="4" fill="#3b82f6"/>
  <text x="360" y="175" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff">Primary</text>
  <text x="360" y="195" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">#3b82f6</text>

  <!-- Accent color -->
  <rect x="450" y="150" width="120" height="60" rx="4" fill="#f59e0b"/>
  <text x="510" y="175" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff">Accent</text>
  <text x="510" y="195" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">#f59e0b</text>

  <!-- Neutral colors -->
  <rect x="600" y="150" width="120" height="60" rx="4" fill="#64748b"/>
  <text x="660" y="175" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff">Neutral</text>
  <text x="660" y="195" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Auto</text>

  <!-- Color scales -->
  <text x="600" y="280" font-family="Arial" font-size="24" font-weight="bold" text-anchor="middle" fill="#1e293b">Color Scales</text>

  <!-- Primary Color Scale -->
  <g transform="translate(150, 320)">
    <rect x="0" y="0" width="80" height="40" rx="4" fill="#eff6ff"/>
    <rect x="90" y="0" width="80" height="40" rx="4" fill="#dbeafe"/>
    <rect x="180" y="0" width="80" height="40" rx="4" fill="#bfdbfe"/>
    <rect x="270" y="0" width="80" height="40" rx="4" fill="#93c5fd"/>
    <rect x="360" y="0" width="80" height="40" rx="4" fill="#60a5fa"/>
    <rect x="450" y="0" width="80" height="40" rx="4" fill="#3b82f6"/>
    <rect x="540" y="0" width="80" height="40" rx="4" fill="#2563eb"/>
    <rect x="630" y="0" width="80" height="40" rx="4" fill="#1d4ed8"/>
    <rect x="720" y="0" width="80" height="40" rx="4" fill="#1e40af"/>
    <rect x="810" y="0" width="80" height="40" rx="4" fill="#1e3a8a"/>
  </g>

  <!-- Accent Color Scale -->
  <g transform="translate(150, 380)">
    <rect x="0" y="0" width="80" height="40" rx="4" fill="#fffbeb"/>
    <rect x="90" y="0" width="80" height="40" rx="4" fill="#fef3c7"/>
    <rect x="180" y="0" width="80" height="40" rx="4" fill="#fde68a"/>
    <rect x="270" y="0" width="80" height="40" rx="4" fill="#fcd34d"/>
    <rect x="360" y="0" width="80" height="40" rx="4" fill="#fbbf24"/>
    <rect x="450" y="0" width="80" height="40" rx="4" fill="#f59e0b"/>
    <rect x="540" y="0" width="80" height="40" rx="4" fill="#d97706"/>
    <rect x="630" y="0" width="80" height="40" rx="4" fill="#b45309"/>
    <rect x="720" y="0" width="80" height="40" rx="4" fill="#92400e"/>
    <rect x="810" y="0" width="80" height="40" rx="4" fill="#78350f"/>
  </g>

  <!-- Neutral Color Scale -->
  <g transform="translate(150, 440)">
    <rect x="0" y="0" width="80" height="40" rx="4" fill="#f8fafc"/>
    <rect x="90" y="0" width="80" height="40" rx="4" fill="#f1f5f9"/>
    <rect x="180" y="0" width="80" height="40" rx="4" fill="#e2e8f0"/>
    <rect x="270" y="0" width="80" height="40" rx="4" fill="#cbd5e1"/>
    <rect x="360" y="0" width="80" height="40" rx="4" fill="#94a3b8"/>
    <rect x="450" y="0" width="80" height="40" rx="4" fill="#64748b"/>
    <rect x="540" y="0" width="80" height="40" rx="4" fill="#475569"/>
    <rect x="630" y="0" width="80" height="40" rx="4" fill="#334155"/>
    <rect x="720" y="0" width="80" height="40" rx="4" fill="#1e293b"/>
    <rect x="810" y="0" width="80" height="40" rx="4" fill="#0f172a"/>
  </g>

  <!-- Preview Section -->
  <rect x="200" y="500" width="800" height="100" rx="8" fill="#ffffff" stroke="#e2e8f0" stroke-width="2"/>
  <text x="600" y="540" font-family="Arial" font-size="18" font-weight="bold" text-anchor="middle" fill="#1e293b">Design System Preview</text>
  <text x="600" y="570" font-family="Arial" font-size="14" text-anchor="middle" fill="#64748b">See how colors work together in UI components</text>
</svg>